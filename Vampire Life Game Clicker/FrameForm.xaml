<Window x:Class="Vampire_Life_Game_Clicker.FrameForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Vampire_Life_Game_Clicker"
        mc:Ignorable="d"
        Title="FrameForm" Height="300" Width="300"
        AllowsTransparency="True"
        WindowStyle="None" WindowState="Maximized">
    <Grid Name="RectGrid">
        <Label Name="Message" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="25" Background="Transparent" Margin="0,50,0,0">Test</Label>
        <Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Name="myCanvas"/>
        <Image HorizontalAlignment="Left" VerticalAlignment="Center" Name="ImageForm"></Image>
    </Grid>
</Window>
